package com.mycompany.randomnumberproject;

import java.util.Random;
import java.util.Scanner;
/**
 *
 * @author NWilber2026
 */
public class RandomNumberProject {

    public static void main(String[] args) {
//Scan for Imput:

        Scanner scanner = new Scanner(System.in);
//Random Number Generator:
        
        //Declare random number generator
        Random generator = new Random();
        
        //First set of numbers
        int num1;       
        int num2;
        int num3;
        num1 = generator.nextInt(8);
        num2 = generator.nextInt(8);
        num3 = generator.nextInt(8);
        
        //Second set of numbers
        int num4thru6;
        num4thru6 = generator.nextInt(656);
        
        //Third set of numbers
        int num7thru10;
        num7thru10 = generator.nextInt(10000);
        
        //Print phone number
        System.out.println("Random Phone Number: " + (num1) + (num2) + (num3) + "-" + (num4thru6) + "-" + (num7thru10));       
      
    }
}
